{
  "name": "the-turn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prisma:seed": "node ./prisma/index.js",
    "prisma:migrate": "node -r dotenv/config --env-file=.env.development ./node_modules/.bin/prisma migrate dev",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,scss, css,md}\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@mui/x-date-pickers": "^8.0.0-beta.3",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/env": "^15.3.0",
    "@prisma/client": "^5.20.0",
    "@react-email/components": "^0.0.36",
    "@reduxjs/toolkit": "^2.2.7",
    "@supabase/supabase-js": "^2.49.4",
    "bcrypt": "^5.1.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "formidable": "^3.5.2",
    "formik": "^2.4.6",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.2.4",
    "next-auth": "^4.24.8",
    "next-connect": "^1.0.0",
    "next-redux-wrapper": "^8.1.0",
    "prisma": "^5.20.0",
    "react": "^18",
    "react-calendar": "^5.0.0",
    "react-dom": "^18",
    "react-icons": "^5.3.0",
    "react-spinners": "^0.14.1",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0",
    "resend": "^4.2.0",
    "sass": "^1.79.3",
    "swiper": "^11.2.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.13",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3"
  }
